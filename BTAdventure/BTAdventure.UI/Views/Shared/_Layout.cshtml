<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Choices</title>
    @Scripts.Render("~/bundles/jquery")
    @Styles.Render("~/Content/css")
