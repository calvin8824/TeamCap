                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav">
                        <li class="navbarItems">@Html.ActionLink("Home", "Start", "Home")</li>
                        <li class="navbarItems">@Html.ActionLink("Main Menu", "UserCommandCentre", "Account")</li>
                        <li class="navbarItems"><h1 class="customHeader">Sharp Adventure Engine<span><br /></span></h1></li>
